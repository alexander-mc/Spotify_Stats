<h2>Upload your Spotify listening history</h2>

<%= form_for @report, url: new_user_report_path(current_user) do |f| %>
    <%= f.text_field :name, placeholder: "Report Name" %><br>
    <%= f.text_field :file_path, placeholder: "Select File" %><br>
    <%= f.submit 'Create' %>
<% end %>

<%= link_to 'Cancel', user_reports_path(current_user) %>